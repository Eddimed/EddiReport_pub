{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "newProjectRoot": "projects",
  "projects": {
    "nightscout-reporter": {
      "i18n": {
        "sourceLocale": "de"
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "budgets": [
                {
                  "maximumError": "10mb",
                  "maximumWarning": "7mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10mb",
                  "maximumWarning": "7mb",
                  "type": "anyComponentStyle"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/web.config",
                {
                  "input": "src/build-replacements/assets/master",
                  "output": "assets/",
                  "glob": "*.json"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false
            },
            "dev": {
              "budgets": [
                {
                  "maximumError": "10mb",
                  "maximumWarning": "7mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10mb",
                  "maximumWarning": "7mb",
                  "type": "anyComponentStyle"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/web.config",
                {
                  "input": "src/build-replacements/assets/dev",
                  "output": "assets/",
                  "glob": "*.json"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "allowedCommonJsDependencies": [
              "jszip",
              "pdfmake",
              "file-saver",
              "pdfjs-dist",
              "fast-sha256",
              "https://apis.google.com/js/api.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/web.config"
            ],
            "index": "src/index.html",
            "inlineStyleLanguage": "scss",
            "main": "src/main.ts",
            "outputPath": "dist/nightrep",
            "polyfills": "src/polyfills.ts",
            "scripts": [
              "src/assets/scripts/pdf.js",
              "src/assets/scripts/pdf.worker.js"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nightscout-reporter:build"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "browserTarget": "nightscout-reporter:build:development"
            },
            "production": {
              "browserTarget": "nightscout-reporter:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "inlineStyleLanguage": "scss",
            "karmaConfig": "karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.spec.json"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "module": "app",
          "skipTests": true,
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "module": "app",
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "module": "app",
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      }
    }
  },
  "version": 1,
  "cli": {
    "analytics": "30a9b09d-7682-4302-916b-8e3f7c176ff9"
  }
}
